module test_utils
   use mod_utils
   use funit
   implicit none

contains

   @test
   subroutine test_uppertolower()

      @assertEqual("a", UpperToLower("A"), "UpperToLower('A')")
   
   end subroutine test_uppertolower

   @test
   subroutine test_lower_to_upper()

      @assertEqual("A", LowerToUpper("a"), "LowerToUpper('a')")
   
   end subroutine test_lower_to_upper

end module test_utils
