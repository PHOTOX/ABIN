#!/bin/bash

OUTPUT=output


export LD_LIBRARY_PATH=$LD_LIBRARY_PATH:/usr/local/programs/common/intel/compiler/11.0/081/lib/intel64:/usr/local/programs/common/intel/compiler/11.0/081/lib/intel64
export KMP_STACKSIZE=12000000
ulimit -s unlimited   ##needed for parallel jobs, because we allocate some large static arrays

echo "Running on node:" > $OUTPUT
uname -n >> $OUTPUT
echo " " >> $OUTPUT

./abin.par >> $OUTPUT

